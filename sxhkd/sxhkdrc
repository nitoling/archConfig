super + Return
	sakura
super + space
	rofi -show
super + r
   rofi -show run
super + q
	bspc node -c

Print
    deepin-screenshot

super + {Up,Bottom,Left,Right}
	bspc node -f {north,south,west,east}

# Volume
XF86AudioMute
	amixer set Master toggle
XF86AudioLowerVolume
	amixer set Master 5%-
XF86AudioRaiseVolume
	amixer set Master 5%+

# restart sxhkd (reload keybinds)
super + shift + r
	pkill -USR1 -x sxhkd

# quit bspwm normally
super + shift + q
	bspc quit

# swap biggest node
super + g
	bspc node -s biggest

# focus the last node/desktop
super + Tab
	bspc desktop -f last

# focus or send to the given desktop
super + 1
	bspc desktop -f 'terminal' --follow
super + 2
	bspc desktop -f 'browser' --follow
super + 3
	bspc desktop -f 'editor' --follow
super + 4
	bspc desktop -f 'slack' --follow
super + 5
	bspc desktop -f 'media' --follow
super + 6
	bspc desktop -f 'other' --follow
